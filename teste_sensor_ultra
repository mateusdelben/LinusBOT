#include <PETEletrica.h>

int trigger_pin = 9, echo_pin = 10

float d = 0;

SensorUltrassonico ultrassonico(trigger_pin,echo_pin);
void setup(){
 Serial Begin(9600);

}
void loop() {
  long microssec = ultrassonico.timing();
  d = ultrassonico.convert(microssec, SensorUltrassonico::CM);
  delay(10);
  Serial.print("Distancia em CM: ");
  Serial.print(d);
  delay(250);
}
